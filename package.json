{
  "name": "@marvinroger/dev-core",
  "version": "1.9.2",
  "description": "This module provides tooling to lint, format, test and write JS modules.",
  "homepage": "https://github.com/marvinroger/js-dev-core",
  "bugs": "https://github.com/marvinroger/js-dev-core/issues",
  "author": {
    "name": "Marvin ROGER",
    "email": "dev@marvinroger.fr",
    "url": "https://www.marvinroger.fr"
  },
  "license": "GPL-3.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist/ && tsc --project ./tsconfig.self.json",
    "lint": "dev-core lint",
    "test": "dev-core test",
    "commit": "dev-core run git-cz",
    "release": "dev-core run standard-version",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "bin": {
    "dev-core": "./dist/cli/index.js"
  },
  "dependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "commander": "^2.19.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "mrm": "^1.2.1",
    "mrm-core": "^3.3.0",
    "prettier": "^1.17.0",
    "standard-version": "^5.0.2",
    "ts-jest": "^24.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.4"
  },
  "devDependencies": {
    "@marvinroger/dev-core": "^1.7.1",
    "rimraf": "^2.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "files": [
    "assets/",
    "dist/",
    "tsconfig.json"
  ]
}
